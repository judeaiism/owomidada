{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error"] }]
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "no-console": ["error", { "allow": ["warn", "error"] }]
      },
      "env": {
        "production": true
      }
    }
  ]
}
